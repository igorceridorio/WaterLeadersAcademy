<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:stretchColumns="1,2,3,4" >

    
    <TableRow android:id="@+id/report_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="20dp"
        android:paddingTop="5dp" >
        
            
        <Button
        android:id="@+id/report_homeButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:text="@string/home_button"
        android:onClick="onClickMainMenu"/>
        
        <TextView android:id="@+id/report"
            android:layout_span="4"
            android:layout_gravity="center"
            android:text="@string/title_report"></TextView> 
    </TableRow>
            
    <TableRow android:id="@+id/header_row"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="10dp" >
        
        <TextView android:id="@+id/activity_header"
            android:layout_width="match_parent"
            android:layout_weight="2"
            android:layout_gravity="center"
            android:text="@string/activity"></TextView>
        
        <TextView android:id="@+id/total_gal_header"
            android:layout_width="match_parent"
            android:layout_weight="1"
            android:layout_gravity="center"
            android:text="@string/total_gal"></TextView>
        
        <TextView android:id="@+id/total_lit_header"
            android:layout_width="match_parent"
            android:layout_weight="1"
            android:layout_gravity="center"
            android:text="@string/total_lit"></TextView>
    </TableRow>
    
    <TableRow android:id="@+id/bathroomSink_row"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >
        
