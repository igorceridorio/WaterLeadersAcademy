            android:layout_width="match_parent"
            android:layout_weight="2"
            android:text="@string/bathroomSink_report"
            android:paddingLeft="6dp"></TextView>

        <TextView android:id="@+id/bathroomSink_total_gal"
            android:layout_width="match_parent"
            android:layout_weight="1"
            android:layout_gravity="center"
            android:text="@string/xx"></TextView>
        
        <TextView android:id="@+id/bathroomSink_total_lit"
            android:layout_width="match_parent"
            android:layout_weight="1"
            android:layout_gravity="center"
            android:text="@string/xx"></TextView>
    </TableRow>
    
