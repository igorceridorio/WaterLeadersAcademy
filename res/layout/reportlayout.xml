    </TableRow>
                
    <TableRow android:id="@+id/finalTotal_row"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="25dp"
        android:paddingTop="10dp" >
        
        <TextView android:id="@+id/finalTotal_report"
            android:layout_width="match_parent"
            android:layout_weight="2"
            android:text="@string/total"
            android:paddingLeft="6dp"></TextView>
            
		<TextView android:id="@+id/finalTotal_total_gal"
            android:layout_width="match_parent"
            android:layout_weight="1"
            android:layout_gravity="center"
            android:text="@string/xx"></TextView>
        
        <TextView android:id="@+id/finalTotal_total_lit"
            android:layout_width="match_parent"
            android:layout_weight="1"
            android:layout_gravity="center"
            android:text="@string/xx"></TextView>
    </TableRow>

    <Button
        android:id="@+id/extras_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/daily_use"
        android:onClick="onClickExtras" />

    <Button
        android:id="@+id/clear_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClickClear"
        android:text="@string/clear_button" />

</TableLayout>
